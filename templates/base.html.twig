						</div>
						<!-- Menu mobile -->
						<div class="collapse navbar-collapse-lg" id="navbarNav">
							<button type="button" class="btn-close" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-label="Close"></button>
							<ul class="navbar-nav">
								<li class="nav-item active">
									<a class="nav-link link-dark" href="/">Home</a>
								</li>
								<li class="nav-item">
									<a class="nav-link link-dark" href="#">Nos Jouets</a>
