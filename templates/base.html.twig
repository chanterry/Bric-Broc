		<link rel="stylesheet" href="./css/nav.css">
		<link rel="stylesheet" href="./css/style.css">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}


	</head>
	<body>
		{% block header %}
			<section id="nav-new-version">
				<nav class="navbar bb__navbar navbar-light navbar-expand-xxl">
					<div class="container-fluid">
